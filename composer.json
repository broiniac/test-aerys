{
  "name": "broiniac/playground",
  "license": "MIT",
  "type": "project",
  "description": "The Playground",
  "autoload": {
    "psr-4": {
      "": "src"
    },
    "classmap": [
        "app/config.php",
        "app/routing.php",
        "app/twig.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests"
    }
  },
  "require": {
    "php": "^7.0",
    "amphp/aerys": "^0.4.3",
    "twig/twig": "~1.0"
  },
  "require-dev": {
    "symfony/finder": "^3.2",
    "symfony/var-dumper": "^3.2",
    "symfony/process": "^3.2"
  }
}
